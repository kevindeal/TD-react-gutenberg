{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "td/content-wrapper",
	"version": "0.1.0",
	"title": "Content Wrapper",
	"category": "text",
	"icon": "id",
	"description": "A wrapper for items in the content block.",
	"attributes": {
		"colorVariant": {
			"type": "string",
			"default": "DARK"
		},
		"imageLoc": {
			"type": "string",
			"default": "right"
		},
		"altImageLoc": {
			"type": "boolean",
			"default": false
		},
		"imageAlignX": {
			"type": "string",
			"default": "center"
		},
		"imageAlignY": {
			"type": "string",
			"default": "center"
		},
		"layout": {
			"type": "string",
			"default": "horizontal"
		},
		"vSplit": {
			"type": "string",
			"default": "50/50"
		},
		"mobileOrder": {
			"type": "string",
			"default": "left-on-top"
		},
		"rowIndex": {
			"type": "number",
			"default": 0
		},
		"imageVAlign": {
			"type":"string",
			"default": "center"
		}
	},
	"example": {},
	"textdomain": "content-wrapper",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}

{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "td/card-container",
	"version": "0.1.0",
	"title": "Card Container",
	"category": "text",
	"icon": "admin-network",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"anchor": true,
		"html": false
	},
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "div.td-alert-text"
		},
		"referenceId": {
			"type":"string"
		},
		"align": {
			"type": "string",
			"default": "none"
		},
		"headingText": {
			"type": "string"
		},
		"iconName": {
			"type": "string"
		},
		"embedded": {
			"type": "boolean",
			"default": false
		},
		"showHeading": {
			"type": "boolean"
		},
		"transparentBackground": {
			"type": "boolean",
			"default": false
		},
		"rows": {
			"type": "number",
			"default": 1
		},
		"columns": {
			"type": "number",
			"default": 3
		},
		"colorVariant": {
			"type": "string",
			"default": "DARK"
		},
		"cardType": {
			"type": "string",
			"default": "td/card-vertical"
		},
		"displayOnMobile" : {
			"type": "boolean",
			"default" : true
		},
		"displayOnTablet" : {
			"type": "boolean",
			"default" : true
		},
		"displayOnDesktop" : {
			"type": "boolean",
			"default" : true
		},
		"columnsOnMobile": {
			"type": "number",
			"default": 2
		},
		"columnsOnTablet": {
			"type": "number",
			"default": 3
		},
		"columnsOnDesktop": {
			"type": "number",
			"default": 4
		},
		"buttonText": {
			"type": "string",
			"default": "Get a demo"
		},
		"buttonUrl": {
			"type": "string",
			"default": ""
		},
		"buttonStyle": {
			"type": "string",
			"default": "hide"
		},
		"buttonTargetBlank": {
			"type": "boolean",
			"default": false
		}
	},
	"example": {},
	"textdomain": "card-container",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
